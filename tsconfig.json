{
    "exclude": [
      "node_modules/*/**",
      "apollo/*/**",
      "dist/**/*",
    ],
    "compilerOptions": {
      "target": "es2020",
      "module": "commonjs",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "outDir": "./dist",
      "rootDir": "schema",
      "declaration": true
    }
  }